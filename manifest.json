{
  "manifest_version": 3,
  "name": "Stock Log",
  "description": "Check the prices of stocks",
  "version": "1.0.0",
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons":{"128": "icon_128.png"},
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "bundle.js"
  },
  "sandbox": {
    "pages":["sandbox.html"]
  }
}